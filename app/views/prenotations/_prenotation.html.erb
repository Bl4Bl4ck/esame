<div id="<%= dom_id prenotation %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Nome:</strong>
    <%= prenotation.nome %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Cognome:</strong>
    <%= prenotation.cognome %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Partenza:</strong>
    <%= prenotation.partenza %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Brasile:</strong>
    <%= prenotation.brasile? ? "Si" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Argentina:</strong>
    <%= prenotation.argentina? ? "Si" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Peru:</strong>
    <%= prenotation.peru? ? "Si" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Colombia:</strong>
    <%= prenotation.colombia? ? "Si" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Cile:</strong>
    <%= prenotation.cile? ? "Si" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Carrozzina:</strong>
    <%= prenotation.carrozzina? ? "Si" : "No" %>
      <div>
    <strong class="block font-medium mb-1">Persone:</strong>
    <%= prenotation.persone %>
  </div>
</div>
